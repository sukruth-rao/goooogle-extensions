{
    "manifest_version" : 3,
    "name" : "Reality Check",
    "description" : "Level 0 extension",
    "version" : "1.0",
    "action" : {
        "default_popup" : "hello.html",
        "default_icon" : "hello_extensions.png"
    },

    "content_script" : [
      {
        "js" : ["scripts/content.js"],
        "matches" : [
          "https://*.youtube.com/*",
          "https://developer.chrome.com/docs/extensions/*",
          "https://developer.chrome.com/docs/webstore/*"
        ]
      }
    ]

}
